translate schinese chapter6_5ecd0c99:

    # cyr "...Such as?"
    cyr "...比如？"

# game/script.rpy:10001
translate schinese chapter6_bb11401a:

    # Ax "Uhh... Well..."
    Ax "呃啊...好吧..."

